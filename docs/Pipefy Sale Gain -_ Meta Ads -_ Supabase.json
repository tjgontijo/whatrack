    },
    {
      "parameters": {
        "jsCode": "// Pega os dados dos nós anteriores\nconst set = $('set').item.json;\nconst inbound = $('get_inbound').first().json;\nconst ph = $('sha256_ph').first().json.ph;\nconst fn = $('sha256_fn').first().json.fn;\nconst ln = $('sha256_ln').first().json.ln;\nconst em = $('sha256_em').first().json.em;\nconst services = set.services || [];\n\n// Ajuste da data para UTC-3 e formato ISO (Meta exige UTC no formato ISO 8601)\nconst date = new Date();\ndate.setHours(date.getHours() - 3);\nconst purchase_time = date.toISOString();\n\n// Soma dos valores dos serviços\nconst amount = services.reduce((total, item) => {\n  const raw = item.Valor || \"0\";\n  const normalized = raw.replace(/\\./g, \"\").replace(\",\", \".\");\n  const num = parseFloat(normalized) || 0;\n  return total + num;\n}, 0);\n\n// Arredonda para 2 casas decimais\nconst formattedAmount = parseFloat(amount.toFixed(2));\n\nreturn [{\n  json: {\n    ph,\n    fn,\n    ln,\n    em,   \n    pixel: set.pixel || null,\n    inbound_id: inbound.id,\n    order_id: set.card_id,\n    purchase_time,\n    amount: formattedAmount,\n    ctwaclid: inbound.ctwaclid,\n    services\n  }\n}];\n"
      },
